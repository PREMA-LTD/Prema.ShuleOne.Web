    "keycloak-js": "^25.0.6",
    "moment": "^2.30.0",
    "ng-matero": "^18.1.0",
    "ngx-permissions": "^17.1.0",
    "ngx-progressbar": "^12.0.0",
    "ngx-toastr": "^19.0.0",
